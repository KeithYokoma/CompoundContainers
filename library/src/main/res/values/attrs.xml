<?xml version="1.0" encoding="utf-8"?>
<resources>
    <attr name="defaultChecked" format="boolean|reference"/>
    <declare-styleable name="CompoundRelativeLayout">
        <attr name="defaultChecked"/>
    </declare-styleable>
    <declare-styleable name="CompoundFrameLayout">
        <attr name="defaultChecked"/>
    </declare-styleable>
    <declare-styleable name="CompoundGridLayout">
        <attr name="defaultChecked"/>
    </declare-styleable>
    <declare-styleable name="CompoundLinearLayout">
        <attr name="defaultChecked"/>
    </declare-styleable>
    <attr name="radioButtonStyle" format="reference" />
    <attr name="rg_orientation" format="enum">
        <enum name="horizontal" value="0"/>
        <enum name="vertical" value="1"/>
    </attr>
    <declare-styleable name="RadioGroup2">
        <!-- The id of the child radio button that should be checked by default
             within this radio group. -->
        <attr name="checkedButton" format="integer" />
        <!-- Should the radio group be a column or a row?  Use "horizontal"
             for a row, "vertical" for a column.  The default is
             vertical. -->
        <attr name="rg_orientation" />
    </declare-styleable>
</resources>